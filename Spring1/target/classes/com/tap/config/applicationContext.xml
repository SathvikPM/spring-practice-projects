<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
             <!-- to create a object -->
    <bean class="com.tap.pojo.Employee" id="empI1">
           <!-- to give the values -->
    <property name="id" value="1"></property>
    <property name="name" value="tim"></property>
    <property name="email" value="tim@gmail.com"></property>
    <property name="salary" value="60000"></property>
 
    </bean>

    <bean class="com.tap.pojo.Employee" id="empI2">
     <property name="id" value="2"></property>
    <property name="name" value="steve"></property>
    <property name="email" value="steve@gmail.com"></property>
    <property name="salary" value="70000"></property>
    </bean>
    
</beans>